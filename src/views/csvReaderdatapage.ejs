<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>CSV Data Page</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
    <link href="/csvReaderdatapage.css" rel="stylesheet" type="text/css" />
    <script
      src="https://kit.fontawesome.com/4747e8ca62.js"
      crossorigin="anonymous"
    ></script>
  </head>
  <body>
    <div id="mainContainer">
      <div id="headerDiv"></div>
      <div id="searchDiv">
        <div class="dropdown">
          <button
            id="filterDropdown"
            class="btn btn-secondary dropdown-toggle"
            type="button"
            data-bs-toggle="dropdown"
            aria-expanded="false"
          >
            <i class="fa-solid fa-filter"></i> Header:
            <span id="filterValue"></span>
          </button>
          <ul class="dropdown-menu" id="headerFilterdropdown">
            <% Object.keys(csvData[1]).forEach(function(key) { %> <%if(key!=
            "__index" && key!= "$__parent" && key!= "$__" && key!= "_doc" &&
            key!= "$isNew" && key!= "__parentArray" ){%>
            <li class="dropdown-item newheader"><%= key %></li>
            <% } %> <% }); %>
          </ul>
        </div>
        <div id="searchBytext">
          <input
            type="text"
            id="searchBytextInput"
            placeholder="Search by text"
          />
        </div>
        <div id="searchButton">
          <button>Search</button>
        </div>
      </div>
      <div id="csvdataTablediv">
        <table id="csvdataTable">
          <thead class="tableHead">
            <tr>
              <% Object.keys(csvData[1]).forEach(function(key) { %> <%if(key!=
              "__index" && key!= "$__parent" && key!= "$__" && key!= "_doc" &&
              key!= "$isNew" && key!= "__parentArray" ){%>
              <th scope="col"><%= key %></th>
              <% } %> <% }); %>
            </tr>
          </thead>
          <tbody>
            <% csvData.forEach(function(row) { %>
            <tr>
              <% Object.keys(row).forEach(function(key) { %> <% if (key !==
              '__index' && key !== '$__parent' && key !== '$__' && key !==
              '_doc' && key !== '$isNew' && key !== '__parentArray') { %>
              <td><%= row[key] %></td>
              <% } %> <% }); %>
            </tr>
            <% }); %>
          </tbody>
        </table>
      </div>
    </div>
  </body>
  <script
    src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
    crossorigin="anonymous"
  ></script>
  <script src="/csvReaderdatapage.js"></script>
  <script src="https://d3js.org/d3.v7.min.js"></script>
</html>
